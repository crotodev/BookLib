        <dc:title>Sword of Destiny</dc:title>
        <dc:creator opf:file-as="Sapkowski, Andrzej &amp; French, David" opf:role="aut">Andrzej Sapkowski</dc:creator>
        <dc:creator opf:file-as="Sapkowski, Andrzej &amp; French, David" opf:role="aut">David French</dc:creator>
