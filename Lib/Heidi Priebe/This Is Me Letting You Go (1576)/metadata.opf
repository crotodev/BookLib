        <dc:title>This Is Me Letting You Go</dc:title>
        <dc:creator opf:file-as="Priebe, Heidi &amp; Catalog, Thought" opf:role="aut">Heidi Priebe</dc:creator>
        <dc:creator opf:file-as="Priebe, Heidi &amp; Catalog, Thought" opf:role="aut">Thought Catalog</dc:creator>
